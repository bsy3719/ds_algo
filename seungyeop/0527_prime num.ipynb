   "source": [
    "# 소수찾기"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(n):\n",
    "    answer = 0\n",
    "    \n",
    "    number = [0,0] + [1]*(n-1)\n",
    "\n",
    "    for i in range(2,n+1):\n",
    "        if number[i]:\n",
    "            answer = answer + 1\n",
    "\n",
    "        for j in range(2*i, n+1, i):\n",
    "            number[j] = 0\n",
    "    \n",
    "    return answer"
   ]
